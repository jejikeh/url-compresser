{
  "ApplicationConfiguration": {
    "RequireUniqueUrls": true,
    "AllowSearchByInitialUrl": true,
    "ChangeSmallUrlOnUpdate": false
  },
  "DatabaseConfiguration": {
    "Provider": "Sqlite",
    "ConnectionString": "Data Source=UrlCompressor.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
